{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
	"rules": {
		"no-alert": 0,
		"no-bitwise": 0,
		"camelcase": 1,
		"curly": 1,
		"eqeqeq": 0,
		"no-eq-null": 0,
		"guard-for-in": 1,
		"no-empty": 1
	},
	"globals": {
		"$": false
	},
	"env": {
		"browser": true,
		"node": true
	}
}